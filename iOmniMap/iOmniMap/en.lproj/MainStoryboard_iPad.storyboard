                        <subviews>
                            <textView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleToFill" text="iOmniMap allows you to navigate in any map that you can download or photograph." textAlignment="center" id="5e3-fP-ss0">
                                <rect key="frame" x="0.0" y="38" width="768" height="34"/>
                                <autoresizingMask key="autoresizingMask" widthSizable="YES" heightSizable="YES"/>
                                <color key="backgroundColor" red="1" green="1" blue="1" alpha="1" colorSpace="custom" customColorSpace="sRGB"/>
